#include <iostream>
#include <map>
#include <unordered_map>
using namespace std;

int main()
{
    map<int, string> m{{1, "apple"}, {3, "banana"}, {2, "cherry"}};
    cout << "map: ";
    for (auto &p : m)
    cout << p.first << "->" << p.second << " ";
    cout << endl;
    unordered_map<int, string> um{{1, "red"}, {2, "green"}, {3, "blue"}};
    cout << "unordered_map: ";
    for (auto &p : um)
    cout << p.first << "->" << p.second << " ";
    cout << endl;
    multimap<int, string> mm{{1, "cat"}, {1, "dog"}, {2, "bird"}};
    cout << "multimap: ";
    for (auto &p : mm)
     cout << p.first << "->" << p.second << " ";
    cout << endl;
  return 0;
}
